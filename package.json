{
  "name": "mocha-nock",
  "version": "1.0.2",
  "description": "Easy recording and playback of HTTP(S) for Mocha.js using Nock.",
  "main": "dist/describe-fixture.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && rm -rf test/fixtures && node_modules/mocha/bin/mocha test/*-test.js ",
    "manual-release": "semantic-release --no-ci",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unboundedsystems/mocha-nock.git"
  },
  "keywords": [
    "mocha",
    "nock",
    "testing",
    "fixtures",
    "mocking"
  ],
  "author": "Mark Terrel",
  "contributors": [
    "Jake Craige <jake@poeticsystems.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unboundedsystems/mocha-nock/issues"
  },
  "engines": {
    "node": ">=10.13"
  },
  "dependencies": {
    "fs-extra": "^8.0.0",
    "lodash.assign": "^4.2.0",
    "mkdirp": "^1.0.0",
    "nock": "^13.0.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/fs-extra": "8.1.5",
    "@types/lodash.assign": "4.2.8",
    "@types/mkdirp": "1.0.2",
    "@types/mocha": "5.2.7",
    "@types/nock": "10.0.3",
    "@types/node": "10.17.60",
    "express": "4.18.2",
    "mocha": "3.5.3",
    "proxyquire": "2.1.3",
    "request": "2.88.2",
    "semantic-release": "17.4.7",
    "typescript": "4.9.5"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
